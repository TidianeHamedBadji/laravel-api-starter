{
    "name": "tidiane-hamed/laravel-api-starter",
    "description": "ðŸš€ Laravel API Starter Kit - Bootstrap professional Laravel API applications with best practices and conventions. CLI tool for rapid API development.",
    "type": "library",
    "keywords": ["laravel", "api", "starter-kit", "cli", "backend", "rest", "api-development"],
    "homepage": "https://github.com/TidianeHamedBadji/laravel-api-starter",
    "license": "MIT",
    "authors": [
        {
            "name": "Tidiane Hamed Badji",
            "email": "tidiane.hamedbadji@gmail.com",
            "homepage": "https://github.com/TidianeHamedBadji",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TidianeHamedBadji/laravel-api-starter/issues",
        "source": "https://github.com/TidianeHamedBadji/laravel-api-starter"
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "symfony/console": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "TidianeHamed\\LaravelApiStarter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TidianeHamed\\LaravelApiStarter\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/laravel-api"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "TidianeHamed\\LaravelApiStarter\\ApiStarterServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}